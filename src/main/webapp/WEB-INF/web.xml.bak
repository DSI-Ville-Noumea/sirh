	<context-param>
		<param-name>REPERTOIRE_MODELES_CONTRATS</param-name>
		<param-value>file://///ysi/datacentre$/DRH/SIRH-document/DEV/Modeles_Documents/Contrats/</param-value>
		<description></description>
	</context-param>
	<context-param>
		<param-name>REPERTOIRE_MODELES_FICHEPOSTE</param-name>
		<param-value>file://///ysi/datacentre$/DRH/SIRH-document/DEV/Modeles_Documents/FichePoste/</param-value>
		<description></description>
	</context-param>
	<context-param>
		<param-name>REPERTOIRE_MODELES_AFFECTATIONS</param-name>
		<param-value>file://///ysi/datacentre$/DRH/SIRH-document/DEV/Modeles_Documents/Affectations/</param-value>

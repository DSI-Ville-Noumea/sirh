	</context-param>
	<context-param>
		<param-name>REPERTOIRE_ROOT</param-name>
		<param-value>//ysi/datacentre$/DRH/SIRH-document/DEV/Document_Associe/</param-value>
		<description></description>
	</context-param>
	<context-param>
		<param-name>VERSION</param-name>
		<param-value>1.0.9</param-value>
		<description></description>
	</context-param>
	<context-param>
		<param-name>STARJET_MODE</param-name>
		<param-value>DVLP</param-value>
		<description></description>
	</context-param>

	<!-- Authentification ldap -->
	<context-param>
		<param-name>CRITERE_RECHERCHE_LDAP</param-name>
		<param-value>samaccountname</param-value>
		<description></description>
	</context-param>
	<context-param>
		<param-name>HOST_LDAP_ADMIN</param-name>
		<param-value>cn=adminwas,ou=WAS,ou=APPLI,ou=Z-users</param-value>
		<description></description>
	</context-param>
	<context-param>
		<param-name>HOST_LDAP</param-name>
		<param-value>ldap://hurle:389/ ldap://gberge:389/ ldap://netone:389/</param-value>
		<description></description>
	</context-param>
	<context-param>
		<param-name>INITCTX_LDAP</param-name>
		<param-value>com.sun.jndi.ldap.LdapCtxFactory</param-value>
		<description></description>
	</context-param>
	<context-param>
		<param-name>HOST_LDAP_PASSWORD</param-name>
		<param-value>***REMOVED***</param-value>
		<description></description>
	</context-param>
	<context-param>
		<param-name>BASE_LDAP</param-name>
		<param-value>dc=site-mairie,dc=noumea,dc=nc</param-value>
		<description></description>
	</context-param>

	<servlet>
		<servlet-name>GestionAgent</servlet-name>
		<display-name>GestionAgent</display-name>
		<description></description>
		<servlet-class>
			nc.mairie.gestionagent.servlets.ServletAgent</servlet-class>
		<load-on-startup>1</load-on-startup>
	</servlet>
	<servlet-mapping>
		<servlet-name>GestionAgent</servlet-name>
		<url-pattern>/GestionAgent</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>GestionAgent</servlet-name>
		<url-pattern>/GestionAgentServlet</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>Personnel.jsp</welcome-file>
	</welcome-file-list>

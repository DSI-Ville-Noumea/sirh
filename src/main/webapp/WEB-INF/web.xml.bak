<web-app id="WebApp_ID">
	<display-name>Mairie GestionAgent</display-name>
	<context-param>
		<param-name>HOST_SGBD</param-name>
		<!--<param-value>robinnw</param-value>-->
		<param-value>newrobin</param-value>
		<description></description>
	</context-param>
	<context-param>
		<param-name>HOST_SGBD_ADMIN</param-name>
		<param-value>adminwas</param-value>
